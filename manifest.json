{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "Fetch weather data from OpenWeather API",
  "permissions": [
    "activeTab",
    "https://api.openweathermap.org/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img.png"
  }
}
